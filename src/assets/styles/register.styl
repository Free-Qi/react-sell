#register-big
  width 1200px
  height 800px
  margin auto
  margin-top 100px
  overflow hidden

#register-left
  float left
  width 500px
  text-align center

#register-left h2 b
  text-align center
  color black

#register-line
  height 1px
  background-color rgb(229,229,229)
  margin-top 20px

#register-input
  position relative

#register-code
  position: absolute
  width 70px
  height 28px
  right 75px
  top 70px
  cursor pointer

//.register-phone
//  position absolute
