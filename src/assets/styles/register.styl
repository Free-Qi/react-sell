//  font-size 10px
//  left 80px
//  top 30px
//  display none

#register-dynamicState
  position absolute
  color rgb(255,102,64)
  font-size 15px
  right 70px
  top 140px
  cursor pointer

#register-code img
  width 100%
  height 100%

#register-input>input
  display block
  width 360px
  height 35px
  margin auto
  border-radius 5px
  outline none
  margin-top 30px
  font-size 15px
  border 1px solid #ccc
  font-weight lighter
  padding-left 20px


#register-agree>span
  color black

#register-btn
  background-color rgb(255,80,0)
  font-size 15px
  color white
  width 380px
  margin 20px auto
  text-align center
  padding 10px 0
  outline none
  border-radius 7px
  border none
  cursor pointer

#register-clause
  font-size 15px

#register-clause>div
  display inline-block

#register-right
  float right
  width 600px
  text-align center

#register-backimg
  width 400px
  height 460px
  margin auto
  margin-top 30px

#register-backimg img
  width 100%
  height 100%