*
  margin: 0
  padding: 0
  box-sizing: border-box
body
  width 100%
  height 100%
.clearboth
  clear: both
.container
  width 1200px
  height 100%
  margin auto
.bigborder
  border 10px solid #e5e5e5
  border-radius 20px
#m-login
  width: 100%
  height:500px
  position: relative
  background: url("https://pic.maizuo.com/manager/6f214638a922e26b818004d8eebc03c3.jpg") center center no-repeat
  border: 1px solid black
#login-right
  width: 370px
  height: 390px
  border: 10px solid #e5e5e5
  border-radius: 20px
  text-align: center
  padding: 20px 20px 0
  position: absolute
  top:90px
  right: 120px
#login-right-top
  height: 38px
  border-bottom: 1px solid #ccc
  margin-bottom: 20px
  padding-bottom: 10px
  text-align: center
.active
  color: #ff6640;
#login-right-top span
  display: inline-block
  font-size: 20px
  font-weight: bold
  width: 125px
  height: 27px
  cursor: pointer
#login-right-top span:nth-child(2)
  border-left: 1px solid #ccc
#input1
  text-align: center
.input1
  height: 18px
  width: 200px
  outline: none
  border: none
.bor
  text-align: left
  width: 300px
  height: 42px
  padding: 10px
  border-radius: 3px
  border: 1px solid gainsboro
  margin: 10px
#loginbtn
  margin-top: 20px
  color: white
  font-size: 15px
  width: 300px
  background-color: rgb(255,80,0)
  padding: 15px 0
  outline: none
  border: none
  border-radius: 7px
  cursor pointer
#web-nav
  width 100%
  height 60px
  background-color rgb(29,29,29)
  position fixed
  top 0px
  left 0px
  z-index 99999999999999999
#web-logo
  width 95px
  height 60px
  background url('../../assets/Allimg/otherImg/web-logo.png') no-repeat left
  text-align right
  color white
  display inline-block
  vertical-align middle
#city
  width 60px
  height 60px
  position relative
  display inline-block
  vertical-align middle
  color rgb(163,163,163)
  cursor pointer
  margin-left 50px
  line-height 60px
  text-align center

#city img
  width 40%
  height 40%
  vertical-align middle
#allCity
  width 600px
  height 300px
  //overflow scroll
  //overflow-x hidden
  position absolute
  left -200px
  top 60px
  background-color white
  padding: 20px 20px 0



